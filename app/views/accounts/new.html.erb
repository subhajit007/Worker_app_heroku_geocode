<h1>Add a new Account</h1>
<div class="row">

</div>
<%= form_for @account, class: "col s12" do |f| %>
  <div class="row">
    <div class="input-field col s6">
    <%= f.label :name%><%=f.text_field :name %>
    </div>
    <div class="input-field col s6">
      <%= f.label :zipcode %><%= f.text_field :zipcode %>
    </div>
  </div>
  <%= f.button "Submit", class: "btn orange waves-effect waves-light"%>
<% end %>
